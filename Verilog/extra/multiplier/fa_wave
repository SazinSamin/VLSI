    .scope S_0x555cb1a0b040;
T_2 ;
    %delay 10, 0;
    %load/vec4 v0x555cb1a1e120_0;
    %addi 1, 0, 32;
    %store/vec4 v0x555cb1a1e120_0, 0, 32;
    %jmp T_2;

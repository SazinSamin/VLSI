    %store/vec4 v0x555cb1a1da30_0, 0, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x555cb1a0b040;
T_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x555cb1a1e120_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x555cb1a1e1f0_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x555cb1a1de90_0, 0, 1;
    %end;

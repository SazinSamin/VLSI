:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5614785554b0 .scope module, "half_adder_tb" "half_adder_tb" 2 1;
 .timescale 0 0;
v0x561478566a80_0 .var "a", 0 0;
v0x561478566b40_0 .var "b", 0 0;
v0x561478566c10_0 .net "c", 0 0, v0x561478566840_0;  1 drivers
v0x561478566d10_0 .net "s", 0 0, v0x561478566910_0;  1 drivers
S_0x561478555640 .scope module, "inst" "half_adder" 2 7, 3 1 0, S_0x5614785554b0;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "s";
    .port_info 1 /OUTPUT 1 "c";
    .port_info 2 /INPUT 1 "a";
    .port_info 3 /INPUT 1 "b";
v0x56147851d7b0_0 .net "a", 0 0, v0x561478566a80_0;  1 drivers
v0x56147851dba0_0 .net "b", 0 0, v0x561478566b40_0;  1 drivers
v0x561478566840_0 .var "c", 0 0;
v0x561478566910_0 .var "s", 0 0;
E_0x56147851c750 .event edge, v0x56147851d7b0_0, v0x56147851dba0_0;
    .scope S_0x561478555640;
T_0 ;
    %wait E_0x56147851c750;
    %load/vec4 v0x56147851d7b0_0;
    %load/vec4 v0x56147851dba0_0;
    %xor;
    %store/vec4 v0x561478566910_0, 0, 1;
    %load/vec4 v0x56147851d7b0_0;
    %load/vec4 v0x56147851dba0_0;
    %and;
    %store/vec4 v0x561478566840_0, 0, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5614785554b0;
T_1 ;
    %vpi_call 2 15 "$dumpfile", "test_half_adder.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5614785554b0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561478566a80_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561478566b40_0, 0, 1;
    %delay 20, 0;
    %vpi_call 2 18 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x5614785554b0;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x561478566a80_0;
    %inv;
    %store/vec4 v0x561478566a80_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5614785554b0;
T_3 ;
    %delay 10, 0;
    %load/vec4 v0x561478566b40_0;
    %inv;
    %store/vec4 v0x561478566b40_0, 0, 1;
    %jmp T_3;
    .thread T_3;

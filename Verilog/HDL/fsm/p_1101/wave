    %jmp T_0.14;
T_0.13 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x564ed2b2f790_0, 0, 2;
T_0.14 ;
    %jmp T_0.6;
T_0.6 ;
    %pop/vec4 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x564ed2b1a9e0;
T_1 ;
    %wait E_0x564ed2ade870;
    %load/vec4 v0x564ed2b2f790_0;
    %pushi/vec4 3, 0, 2;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x564ed2adfba0_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x564ed2b2f600_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %pushi/vec4 0, 0, 1;

:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55f68f586e10 .scope module, "Task" "Task" 2 2;
 .timescale 0 0;
v0x55f68f5cfb60_0 .var "a", 1 0;
v0x55f68f5cfc60_0 .var "b", 1 0;
v0x55f68f5cfd40_0 .var "c", 3 0;
S_0x55f68f586fa0 .scope task, "sum" "sum" 2 3, 2 3 0, S_0x55f68f586e10;
 .timescale 0 0;
v0x55f68f588350_0 .var "x", 1 0;
v0x55f68f5887b0_0 .var "y", 1 0;
v0x55f68f588ba0_0 .var "z", 3 0;
TD_Task.sum ;
    %load/vec4 v0x55f68f588350_0;
    %pad/u 4;
    %load/vec4 v0x55f68f5887b0_0;
    %pad/u 4;
    %add;
    %store/vec4 v0x55f68f588ba0_0, 0, 4;
    %end;
    .scope S_0x55f68f586e10;
T_1 ;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x55f68f5cfb60_0, 0, 2;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x55f68f5cfc60_0, 0, 2;
    %load/vec4 v0x55f68f5cfb60_0;
    %store/vec4 v0x55f68f588350_0, 0, 2;
    %load/vec4 v0x55f68f5cfc60_0;
    %store/vec4 v0x55f68f5887b0_0, 0, 2;
    %fork TD_Task.sum, S_0x55f68f586fa0;
    %join;
    %load/vec4 v0x55f68f588ba0_0;
    %store/vec4 v0x55f68f5cfd40_0, 0, 4;
    %vpi_call 2 15 "$display", "Result: %d", v0x55f68f5cfd40_0 {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.

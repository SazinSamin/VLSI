#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55c84d0f8b90 .scope module, "lsr_tb" "lsr_tb" 2 1;
 .timescale 0 0;
P_0x55c84d0f8d20 .param/l "N" 0 2 2, +C4<00000000000000000000000000000100>;
v0x55c84d10a910_0 .var "clk", 0 0;
v0x55c84d10a9d0_0 .var "d", 0 0;
v0x55c84d10aaa0_0 .net "out", 3 0, v0x55c84d10a6e0_0;  1 drivers
v0x55c84d10aba0_0 .var "rst", 0 0;
S_0x55c84d0f8dc0 .scope module, "uut" "lsr" 2 6, 3 1 0, S_0x55c84d0f8b90;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "d";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "rst";
    .port_info 3 /OUTPUT 4 "out";
P_0x55c84d0f9830 .param/l "N" 0 3 2, +C4<00000000000000000000000000000100>;
v0x55c84d0becf0_0 .net "clk", 0 0, v0x55c84d10a910_0;  1 drivers
v0x55c84d10a620_0 .net "d", 0 0, v0x55c84d10a9d0_0;  1 drivers
v0x55c84d10a6e0_0 .var "out", 3 0;
v0x55c84d10a7a0_0 .net "rst", 0 0, v0x55c84d10aba0_0;  1 drivers
E_0x55c84d0bf730 .event posedge, v0x55c84d0becf0_0;
    .scope S_0x55c84d0f8dc0;
T_0 ;
    %wait E_0x55c84d0bf730;
    %load/vec4 v0x55c84d10a7a0_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55c84d10a6e0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55c84d10a6e0_0;
    %parti/s 3, 0, 2;
    %load/vec4 v0x55c84d10a620_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x55c84d10a6e0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55c84d0f8b90;
T_1 ;
    %vpi_call 2 14 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55c84d0f8b90 {0 0 0};
    %pushi/vec4 0, 0, 3;
    %split/vec4 1;
    %assign/vec4 v0x55c84d10aba0_0, 0;
    %split/vec4 1;
    %assign/vec4 v0x55c84d10a910_0, 0;
    %assign/vec4 v0x55c84d10a9d0_0, 0;
    %delay 200, 0;
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x55c84d0f8b90;
T_2 ;
    %delay 10, 0;
    %load/vec4 v0x55c84d10a910_0;
    %inv;
    %store/vec4 v0x55c84d10a910_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55c84d0f8b90;
T_3 ;
    %delay 10, 0;
    %load/vec4 v0x55c84d10a9d0_0;
    %pad/u 2;
    %addi 1, 0, 2;
    %pad/u 1;
    %store/vec4 v0x55c84d10a9d0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
